@import "colors.styl"

$main-bg-color = $athens-gray
$main-font-color = $shark

*
    font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif
    font-size: 16px
    line-height: 1.6
    color: $shark
    font-kerning: normal
    box-sizing: border-box

body
    margin: 0
    padding: 0px


a
    text-decoration: none